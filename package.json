{
  "name": "egg-template",
  "version": "1.0.0",
  "description": "template for egg dev with jwt token and mongodb",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.22.2",
    "egg-cors": "^2.2.0",
    "egg-mongo-native": "^3.4.3",
    "egg-morgan": "^1.0.0",
    "egg-scripts": "^2.11.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.13.0",
    "egg-mock": "^3.22.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-egg-template",
    "stop": "egg-scripts stop --title=egg-server-egg-template",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Zhao Chaoyi",
  "license": "MIT"
}
